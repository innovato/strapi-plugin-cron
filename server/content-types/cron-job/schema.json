{
  "kind": "collectionType",
  "collectionName": "cron_jobs",
  "info": {
    "singularName": "cron-job",
    "pluralName": "cron-jobs",
    "displayName": "Cron Job"
  },
  "options": {
    "draftAndPublish": true,
    "comment": ""
  },
  "pluginOptions": {
    "content-manager": {
      "visible": false
    },
    "content-type-builder": {
      "visible": false
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "schedule": {
      "type": "string",
      "required": true
    },
    "script": {
      "type": "text",
      "required": true
    },
    "iterations": {
      "type": "integer",
      "min": -1,
      "default": -1,
      "required": true
    },
    "startDate": {
      "type": "datetime"
    },
    "endDate": {
      "type": "datetime"
    }
  }
}
